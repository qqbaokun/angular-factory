@media screen and (max-width: 1300px) {
    .social,
    .campus,
    .about {
        transform: scale(0.6);
        -webkit-transform: scale(0.6);
        -ms-transform: scale(0.6);
    }
    .social{
    	margin: 0px auto;
    }
    .social-one{
    	margin: 150px auto;
    }
    .campus{
    	margin: 0 auto;
    }
    .campus01{
    	margin: 100px auto;
    } 
}

@media screen and (max-width: 1400px) {
    .social,
    .about {
        transform: scale(0.75);
        -webkit-transform: scale(0.75);
        -ms-transform: scale(0.75);
        transform-origin: 50% 10%;
        -webkit-transform-origin: 50% 10%;
        -ms-transform-origin: 50% 10%;
    }
    .social-one{
        width: 900px;
    }
    .socila-right .slash-small {
		width:290px;
		height:290px;
		margin-top: 58px;
    	margin-left: 65px;
	}
    .social{
    	margin: 0px auto;
    }
    .social-one{
    	margin: 0 auto;
    }
    .campus{
        transform: scale(0.65);
        -webkit-transform: scale(0.65);
        -ms-transform: scale(0.65);
        transform-origin: 50% 10%;
        -webkit-transform-origin: 50% 10%;
        -ms-transform-origin: 50% 10%;
    }
    .social-title, .campus-title, .about-title{
    	margin: 0 auto;
    } 
    .campus{
    	margin: 0 auto;
    }
    .campus01{
    	margin: 100px auto;
    }  
}

@media screen and (min-width:1401px) and (max-width: 1500px) {
    .social,
    .campus,
    .about {
        transform: scale(0.8);
        -webkit-transform: scale(0.8);
        -ms-transform: scale(0.8);
    }
    .social-title, .campus-title, .about-title{
    	margin: 50px auto;
    }
    .social{
    	margin: 0px auto;
    }
    .social-one{
    	margin: 150px auto;
    }
    .campus{
    	margin: 0 auto;
    }
    .campus01{
    	margin: 150px auto;
    }
    .socila-right .slash-small {
		margin-top: 80px;
    	margin-left: 84px;
	}
}

@media screen and (min-width:1501px) and (max-width: 1800px) {
    .social,
    .campus {
        transform: scale(0.9);
        -webkit-transform: scale(0.9);
        -ms-transform: scale(0.9);
    }
    .social-title, .campus-title, .about-title{
    	margin: 50px auto;
    }
    .social{
    	margin: 0px auto;
    }
