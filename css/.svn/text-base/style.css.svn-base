#main-content{
	margin-top: 56px;
}
#home-page {
    position: relative;
}

.social-recruitment,
.campus-recruitment,
.about-us {
    position: relative;
    overflow: hidden;
}
.right-bar {
    position: fixed;
    right: 30px;
    top: 40%;
}

.right-bar-t,
.right-bar-b {
    width: 40px;
    height: 40px;
    background-color: #c4c4c4;
    cursor: pointer;
    text-align: center;
    line-height: 40px;
    font-size: 0;
    position: relative;
}

.right-bar-code {
    width: 148px;
    height: 159px;
    border: 1px solid #eee;
    background-color: white;
    position: fixed;
    right: 80px;
    top: 40%;
    margin-top: -128px;
    z-index: 100;
    display: none;
    padding:5px;
}

.right-bar-code span {
    width: 10px;
    height: 10px;
    display: inline-block;
    position: absolute;
    right: -6px;
    bottom: 15px;
    border-right: 1px solid #eee;
    border-top: 1px solid #eee;
    transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    background-color: white;
}

.right-bar-code img {
    width: 100% !important;
    height: 100% !important;
}

.icon-weixin,
.icon-backtop {
    font-size: 22px;
}

.right-bar div img {
    width: 25px;
    height: 25px;
    vertical-align: middle;
}

.right-bar-b {
    margin-top: 10px;
}


