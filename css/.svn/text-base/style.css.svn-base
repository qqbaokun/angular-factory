/*��ҳ*/

.home-page {
    position: relative;
    z-index: 10;
}

.home-page-bg {
    width: 100%;
    height: 100%;
}

.home-page-bg img {
    width: 100%;
    height: 100%;
}

.home-page-bottom {
    width: 1200px;
    height: 55px;
    position: absolute;
    background-color: white;
    left: 50%;
    margin-left: -600px;
    bottom: 0;
    z-index: 1;
}

.home-page-bottom>div {
    display: inline-block;
}

.home-page-bottom-m {}

.shubiao {
    width: 14px;
    height: 25px;
    position: absolute;
    border: 2px solid #dfdfdf;
    border-radius: 20px;
    left: 50%;
    top: 15px;
    margin-left: -9px;
}

.zhixian {
    width: 2px;
    height: 30px;
    background-color: #dfdfdf;
    margin-left: -1px;
    position: absolute;
    top: 0;
    left: 50%;
    animation: mysecond infinite 1s;
}

.sanjiaoxing {
    transform: rotate(-90deg);
    position: absolute;
    top: 20px;
    left: 50%;
    color: #dfdfdf;
    margin-left: -7px;
    animation: myfirst infinite 1s;
}

@keyframes myfirst {
    0% {
        top: 20px;
    }
    50% {
        top: 26px;
    }
    100% {
        top: 20px;
    }
}

@keyframes mysecond {
    0% {
        height: 30px;
    }
    50% {
        height: 35px;
    }
    100% {
        height: 30px;
    }
}

.home-page-bottom-r {
    position: absolute;
    right: 0;
    bottom: 0;
    cursor: pointer;
}

.weixin-code {
    display: none;
    background-color: white;
}
.weixin-code img{
	width: 100%;
}
.weixin-icon {
    width: 105px;
    height: 55px;
    line-height: 55px;
    text-align: center;
    float: right;
}

.weixin-icon img {
    width: 25px;
    vertical-align: middle;
}

.home-page-bottom-r:hover .weixin-code {
    display: block;
    padding: 5px;
}

.video-bg {
    position: absolute;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .5);
    top: 0;
    text-align: center;
    z-index: 2;
}

.video-content {
    /*margin-top: 200px;*/
    position: relative;
    top: 50%;
    margin-top: -100px;
}

.video-content p:first-child {
    color: white;
    font-size: 60px;
    font-weight: bold;
}

.video-content p:nth-child(2) {
    color: white;
    line-height: 100px;
    font-size: 25px;
}

.video-content a {
    display: inline-block;
    width: 200px;
    height: 50px;
    background-color: #63b504;
    border-radius: 6px;
    color: white;
    line-height: 50px;
    cursor: pointer;
    font-size: 16px;
}
.swiper-container-1 .swiper-wrapper .swiper-slide{
	position:absolute;
	left:0;
	top:0;
}
/*集团化*/
.swiper-container-2{
	border: 1px solid;
    box-sizing: border-box;
    height: 120px;
}
.swiper-container-2 .swiper-slide{
	box-sizing: border-box;
    border-right: 1px solid;
}
.swiper-container-2 .swiper-slide:last-child{
	border-right: none;
}
.collectivize-main{
    max-width: 1200px;
    height: 120px;
    position: absolute;
    z-index: 10;
    bottom: 5%;
    left: 50%;
}
.collectivize-main-li{
    width: 200px;
    height: 100%;
    float:left;
}
.collectivize-main-li img{
    width: 100%;
    height: 100%;
}
.collectivize-main-li span {
	display: inline-block;
	width: 100%;
	text-align: center;
	position: absolute;
	bottom: -50px;
	height: 40px;
    line-height: 40px;
	color:white;
	font-size: 14px;
	left: 0;
	transition:all 0.3s;
	-webkit-transition:all 0.3s;
	-moz-transition:all 0.3s;
	-o-transition:all 0.3s;
}
.swiper-button-prev,.swiper-button-next{
	width: 60px;
    height: 70px;
    margin-top: -30px;
    background-image: none;
}
.swiper-button-prev span,.swiper-button-next span{
	font-size: 70px;
    font-weight: 800;
}
.swiper-button-prev{
    left: -60px; 
}
.swiper-button-next{
    right: -60px;
}
.social,.social-one {
    width: 1200px;
    margin: 0 auto;
    position: relative;
}
.socila-right .slash-small {
	width:400px;
	height:400px;
	margin-top: 80px;
    margin-left: 84px;
}
#footer {
    width: 100%;
    height: 50px;
}


/*placeholder*/

::-webkit-input-placeholder {
    /* WebKit browsers */
    color: #adadad;
}

:-moz-placeholder {
    /* Mozilla Firefox 4 to 18 */
    color: #adadad;
}

::-moz-placeholder {
    /* Mozilla Firefox 19+ */
    color: #adadad;
}

:-ms-input-placeholder {
    /* Internet Explorer 10+ */
    color: #adadad;
}


/*
1024-768
1280-1024
