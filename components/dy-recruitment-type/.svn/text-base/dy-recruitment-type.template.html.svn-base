                background-size: 100%;
                background-attachment: fixed;
                background-repeat: no-repeat;
            }
            
            .campus {
                width: 1200px;
                height: auto;
                position: relative;
            }
            
            .campus-bg {
                height: 100%;
                width: 100%;
                position: absolute;
                background-color: rgba(0, 0, 0, .5);
            }
            
            .square-l {
                width: 275px;
                height: 275px;
                position: absolute;
                cursor: pointer;
                text-transform: uppercase;
                text-align: center;
                display: table;
            }
            
            .square-l-div {
                vertical-align: middle;
                display: table-cell;
            }
            
            .square-l-div p:first-child {
                font-size: 24px;
                margin-bottom: 15px;
            }
            
            .square-l-div p:last-child {
                font-size: 20px;
            }
            
            .square-l-01,
            .square-l-03 {
                background-color: white;
            }
            
            .square-s {
                width: 50px;
                height: 50px;
                position: absolute;
            }
            
            .square-m {
                width: 115px;
                height: 115px;
                position: absolute;
            }
            
            html .square-l:before,
            html .square-l:after,
            .square-l div:before,
            .square-l div:after {
                -webkit-transition: 0.5s;
                transition: 0.5s;
            }
            
            *:before,
            *:after {
                z-index: -1;
            }
            
            .square-l:after {
                content: '';
                width: 0;
                height: 0;
                -webkit-transform: rotate(360deg);
                border-style: solid;
                border-width: 0 0 0 0;
                border-color: transparent #63b504 transparent transparent;
                position: absolute;
                top: 0;
                right: 0;
            }
            
            .square-l:before {
                content: '';
                width: 0;
                height: 0;
                -webkit-transform: rotate(360deg);
                border-style: solid;
                border-width: 0 0 0 0;
                border-color: transparent transparent transparent #63b504;
                position: absolute;
                bottom: 0;
                left: 0;
            }
            
            .square-l:before,
            .square-l:after {
                border-color: #63b504;
            }
            
            .square-l div:after {
                content: '';
                width: 0;
                height: 0;
                -webkit-transform: rotate(360deg);
                border-style: solid;
                border-width: 0 0 0 0;
                border-color: #63b504 transparent transparent transparent;
                position: absolute;
                top: 0;
                left: 0;
            }
            
            .square-l div:before {
                content: '';
                width: 0;
                height: 0;
                -webkit-transform: rotate(360deg);
                border-style: solid;
                border-width: 0 0 0 0;
                border-color: transparent transparent #63b504 transparent;
                position: absolute;
                right: 0;
                bottom: 0;
            }
            
            .square-l div:before,
            .square-l div:after {
                border-color: #63b504;
            }
            
            .square-l:hover {
                background-color: transparent;
            }
            
            .square-l:hover:before {
                border-width: 69px 69px;
            }
            
            .square-l:hover:after {
                border-width: 69px 69px;
            }
            
            .square-l:hover div:before {
                border-width: 69px 69px;
            }
            
            .square-l:hover div:after {
                border-width: 69px 69px;
            }
            
            .square-l:active {
                background: inherit;
            }
        </style>

