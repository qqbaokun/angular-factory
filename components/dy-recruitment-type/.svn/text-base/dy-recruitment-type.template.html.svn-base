<div class="social-recruitment page" ng-if="option.type == 'B1' || option.type == 'B4'">
    <div class="social-title">
        <div class="title-l"><img src="images/title-left.png" alt="" /></div>
        <div class="title-m mc-c">{{option.componentName}}</div>
        <div class="title-r"><img src="images/title-right.png" alt="" /></div>
    </div>
    <!--Start--六边形-->
    <div ng-if="option.type == 'B4'">
        <style>
            .hexagon {
                width: 250px;
                height: 250px;
                position: absolute;
                z-index: 10;
            }
            
            .hexagon-title {
                width: 136px;
                height: 136px;
                position: absolute;
                left: 0;
                top: 192px;
            }
            
            .hexagon-title span {
                display: inline-block;
                position: absolute;
                left: 25px;
                top: 44px;
                font-size: 20px;
                font-weight: bold;
                color: white;
            }
            
            .hexagon-text {
                width: 100%;
                position: absolute;
                left: -10px;
                top: 90px;
                cursor: pointer;
                transition: .2s linear;
                text-align: center;
            }
            
            .hexagon-text div {
                display: inline-block;
            }
            
            .hexagon-text div p {
                font-size: 18px;
                color: #c4c4c4;
                text-align: center;
                word-break: break-all;
            }
            
            .hexagon-text div:first-child {
                font-size: 60px;
                font-weight: 300;
                max-width: 55%;
            }
            
            .hexagon-01 {
                top: 301px;
                left: -79px;
            }
            
            .hexagon-text-01 {}
            
            .hexagon-02 {
                top: 200px;
                left: 95px;
