    <div ng-if="option.type == 'B1'">
        <style type="text/css">
            .social-one {
                /* display: table; */
            }
            
            .socila-left,
            .socila-right {
                display: inline-block;
                width: 588px;
                vertical-align: middle;
                position: relative;
            }
            .socila-right {
            	height: 588px;
            }
            .socila-left-title {
                font-size: 20px;
            }
            
            .socila-left div {
                display: block;
                width: 100%;
                margin: 30px 0;
            }
            
            .socila-left div p {
                line-height: 32px;
            }
            
            .socila-left button {
                display: inline-block;
                width: 200px;
                height: 40px;
                text-align: center;
                line-height: 40px;
                border: 1px solid #63b504;
                border-radius: 6px;
                cursor: pointer;
                background: transparent;
                color: #63b504;
            }
            
            .socila-right .slash-small{
                /*width: 66%;*/
			    border-radius: 100%;
			    display: block;
			    
            }
            
            .socila-left button {
                font-size: 16px;
            }
            
            .slash-big {
                position: absolute;
			    width: 100%;
			    height: 100%;
			    overflow:hidden;
            }
            .mainCircle {
				width:100%;
				height:100%;
				position:absolute;
				z-index:100;
			}
			.secondaryCircle {
				width:100%;
				height:100%;
				position:absolute;
				z-index:100;
			}
			.top_line {
				width:100%;
				height:100%;
				position:absolute;
				z-index:101;
				top: -309px;
    			left: -120px;
			}
			.bottom_line {
				width:60%;
				height:60%;
				position:absolute;
				z-index:101;
				bottom: -42px;
    			right: 65px;
			}
			@media screen and (max-width: 1400px) {
				.socila-left,
            	.socila-right{
            		width:441px;
            	}
            	.socila-right {
            		height:441px;
            	}
            	.top_line {
            		top:-205px;
            	}
            	.bottom_line {
            		bottom: -31px;
    				right: 48px;
            	}
			}
			.mainCircle .st0{fill:<s:property value="configVr.webStyle400TheMainColor"/>;}
			.mainCircle .st1{clip-path:url(#SVGID_2_);}
			.mainCircle .st2{clip-path:url(#SVGID_4_);}
			.top_line .st0{fill:white;}  
			.bottom_line .st0{fill:white;}
        </style>
        <!--svg图-->
        <div class="social-one">
