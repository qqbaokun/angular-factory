            }
            
            .hexagon-04 {
                top: 296px;
                left: 269px;
            }
            
            .hexagon-05 {
                top: 196px;
                left: 443px;
            }
            
            .hexagon-06 {
                top: 297px;
                left: 617px;
            }
            
            .hexagon-07 {
                top: 197px;
                left: 791px;
            }
            
            .hexagon-08 {
                top: 403px;
                left: 791px;
