                	#modifyVolunteerModal .modal-header{border: none;}
                	#modifyVolunteerModal .modal-title{text-align: center;padding: 0;border: none;height: 40px;line-height: 40px}
                	#modifyVolunteerModal .modal-body{padding: 0;padding-bottom: 20px;}
                    #modifyVolunteerModal .main{text-align: center;padding-left: 20px;}
                    #modifyVolunteerModal .main-title{padding-right: 20px;}
                    #modifyVolunteerModal .main-title ul{width: 100%;border-bottom: 1px solid #ccc;}
                    #modifyVolunteerModal .main-title ul li{padding:0 10px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;float: left;height: 35px;line-height: 35px;width: 80px;margin-right: 10px;font-size: 14px;background-color: #F4F3F2;cursor: pointer;}
                    #modifyVolunteerModal .main-title ul li.mc-bgc{color: white;}
                    #modifyVolunteerModal .main-con{text-align: left;width:460px;margin-bottom: 20px;height: 220px;overflow-y: scroll;overflow-x: hidden;}
                    #modifyVolunteerModal .main-con li{margin: 20px 0;}
                    #modifyVolunteerModal .main-con .liTitle{font-size: 14px;line-height: 44px;height: 44px;color: #63b504;}
                    #modifyVolunteerModal li>span{display: inline-block;}
                    #modifyVolunteerModal .liLeft{width: 180px;padding-left: 20px;position: relative;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;vertical-align: middle;}                   
                    #modifyVolunteerModal .icon-bianji{position: absolute;display: block;width: 15px;height: 15px;left: 0;top: 20px;cursor: pointer;}
                    #modifyVolunteerModal .zhiyuan{text-align: center;display: block;width: 56px;height: 20px;line-height: 20px;font-size: 12px;color: #fff;font-style: normal;}
                    #modifyVolunteerModal .liRight{width: 50%;vertical-align: bottom;}
                    #modifyVolunteerModal .liRight a{display: inline-block;width: 18px;height: 18px;line-height: 18px;font-size: 13px;text-align: center;border: 1px solid #ccc;color: #ccc;margin-top: 5px;margin-right: 5px;cursor: pointer;position: relative;}
                    #modifyVolunteerModal .already-selected::after{content: "";border: 3px solid;position: absolute;bottom: 0;right: 0;border-top: 3px solid transparent;border-left: 3px solid transparent;}
                    #modifyVolunteerModal .liRight .cannot-select{cursor: no-drop;border-style: dashed;}
                    #modifyVolunteerModal .btn{display: inline-block;font-size: 14px;color: #2a2a38;margin-top: 30px;width: 319px;height: 44px;line-height: 44px;text-align: center;border-radius: 2px;background-color: #e0e0e0;}
                    #modifyVolunteerModal .main .button{line-height: 38px;width: 200px;background-color: #E2E0DD;color: black;border: none;cursor: auto;}
                    #modifyVolunteerModal .main .button.mc-bgc{color: white;cursor: pointer;}
                    #modifyVolunteerModal .msg-dialog{text-align: left;color: red;font-size: 14px;margin-top: 10px;}

                </style>
                <div class="main">
                	<div class="main-title">
                		<ul class="clearFloat">
	                		<li ng-repeat="a in wishList track by $index" ng-click="chooseResume($index);"; ng-class="{'mc-bgc':$index == resumeWishIndex}" title="{{a.resumeName | replaceText:'其他简历'}}">{{a.resumeName | replaceText:'其他简历'}}</li>	                	
                		</ul>
                	</div> 
                	<div ng-show="canNotEditCampWishNums.length != 0" class="msg-dialog">{{canNotEditCampWishNumsMsg}}</div>              	
                	<div ng-repeat="b in wishList track by $index" ng-if="resumeWishIndex == $index;">
                		<ul class="main-con">
	                        <li class="liTitle">
	                            <span class="liLeft mc-c">{{language.personalCenter.jobCandidates}}</span>
	                            <span class="liRight mc-c">{{language.personalCenter.volunteer}}</span>
	                        </li>
	                        <li class="liContent" ng-repeat="a in b.data track by $index">
	                            <span class="liLeft" title="{{a.name}}"><!-- <i class="iconfont icon-bianji"></i> --><i class="zhiyuan">{{language.personalCenter.numero}}{{a.wishNum | changeNum}}{{language.personalCenter.volunteer}}</i>{{a.name}}</span>
	                            <span class="liRight"><a ng-click="chooseWish(i,$parent.$parent.$index,$parent.$index);" ng-class="{'already-selected mc-c mc-bc':b.newVolunteers[$parent.$index] == $index+1,'cannot-select':isExistInArray(i)}" ng-repeat="i in (campMaxApplyNum > 10 ? 10 : campMaxApplyNum) | createNum">{{i}}</a></span>
	                        </li>
                    	</ul>
	                    <button ng-hide="b.newVolunteers | hasEmptyElement" ng-click="okVolunteer($index,$parent.$index);" class="button button--winona mc-bgc" data-text="{{language.btn.confirmationModification}}">
	                        <span>{{language.btn.confirmationModification}}</span>
	                        <div class="after">{{language.btn.confirmationModification}}</div>
	                    </button>
	                    <button ng-show="b.newVolunteers | hasEmptyElement" class="button button--winona" data-text="{{language.btn.confirmationModification}}">
	                        <span>{{language.btn.confirmationModification}}</span>
	                        <div class="after">{{language.btn.confirmationModification}}</div>
	                    </button>
                	</div>
                    
                </div>
            </div>
        </div>
    </div>
</div>
