<!-- 个人中心 start-->
<div class="person-main clearFloat" ng-if="(listResume.length != 0 || collection.length != 0 || listApplyPosition.length != 0) && modelShow">
    <!-- 左边导航  start-->
    <div class="person-main-left" scroll-to-target>
        <ul class="PML-menu">
            <li ng-if="listApplyPosition.length != 0" class="PML-myApply" click-to-target="myApply" title="{{language.personalCenter.myApplication}}">{{language.personalCenter.myApplication}}</li>
            <li ng-if="listResume.length != 0" class="PML-myResume" click-to-target="myResume" title="{{language.personalCenter.myResume}}">{{language.personalCenter.myResume}}</li>
            <li ng-if="collection.length != 0" class="PML-myCollect" click-to-target="myCollect" title="{{language.personalCenter.myCollection}}">{{language.personalCenter.myCollection}}</li>
            <li ng-if="preachMeeting.length != 0" class="PML-myTeach-in" click-to-target="myTeach-in" title="{{language.personalCenter.myPreachingMeeting}}">{{language.personalCenter.myPreachingMeeting}}</li>
        </ul>
        <dl class="PML-qr">
            <dt>
                <img ng-src="{{wechatQRUrl}}"/>
            </dt>
            <dd>{{language.weChat.attentionWeChat}}</dd>
            <dd>{{language.weChat.getInformation}}</dd>
        </dl>
    </div>
    <!-- 左边导航  end-->
    <!-- 右边主体部分 start-->
    <div class="person-main-right" set-min-height="256">
        <div class="PMR-myApply PML-main" ng-if="listApplyPosition.length != 0">
            <div class="about-title">
                <div class="title-l"><img src="images/title-left.png" alt=""/></div>
                <div class="title-m mc-c">{{language.personalCenter.myApplication}}</div>
                <div class="title-r"><img src="images/title-right.png" alt=""/></div>
            </div>
            <div class="btn-modifyVolunteer" ng-hide="campMaxApplyNum > 10">
                <button ng-show="wishList.length != 0 && canEditCampWishNum == '0'" ng-click="modifyVolunteer();" class="button button--winona mc-c mc-bc" data-text="{{language.personalCenter.modifyVolunteer}}" type="button">
                    <span>{{language.personalCenter.modifyVolunteer}}</span>
                    <div class="after">{{language.personalCenter.modifyVolunteer}}</div>
                </button>
                <button ng-show="positionList.length != 0" ng-click="modifyPosition();" class="button button--winona mc-c mc-bc" data-text="{{language.personalCenter.modifyPosition}}" type="button">
                    <span>{{language.personalCenter.modifyPosition}}</span>
                    <div class="after">{{language.personalCenter.modifyPosition}}</div>
                </button>
            </div>
            <div class="list-modifyVolunteer">
                <table>
                    <thead>
                    <tr>
                        <th width="150">{{language.personalCenter.deliveryTime}}</th>
                        <th width="100">{{language.personalCenter.deliveryPost}}</th>
                        <th width="100">{{language.personalCenter.workplace}}</th>
                        <th width="100">{{language.personalCenter.resumeApplication}}</th>
                        <th width="100">{{language.personalCenter.deliveryNumber}}</th>
                        <th width="100" class="textCen">{{language.personalCenter.applicationSituation}}</th>
                    </tr>
                    </thead>
                    <tbody ng-repeat="a in listApplyPosition track by $index">
                        <tr>
                        <td>
                            <span>{{a.applyDate}}</span>
                            <span class="orange sc-c">（已投递{{a.applyTime}}天）</span></td>
                        <td class="position">
                            <span ng-click="getJobDetail({PostId: a.postId,RecruitType: a.recruitType});" class="no-warp" title="{{a.name}}">{{a.name}}</span>
                            <span class="toRight"></span>
                            <span ng-show="(a.recruitType == 1 || a.recruitType == 13) && a.wishNum && a.wishNum <= campMaxApplyNum" class="icon-volunteer">{{language.personalCenter.numero}}{{a.wishNum | changeNum}}{{language.personalCenter.volunteer}}</span>
                        </td>
                        <td>
                            <span class="no-warp" title="{{a.workPlace}}">{{a.workPlace}}</span>
                        </td>
                        <td>
                            <span class="no-warp mc-c" title="{{a.resumeName}}">【{{a.recruitTypeName}}】{{a.resumeName}}</span>
                        </td>
                        <td>
                            <span class="no-warp" title="{{a.applyCount}}">{{a.applyCount}}</span>
                        </td>
                        <td class="orange">
                            <span class="dispose sc-c" get-apply-detail ng-click="getApplyDetail(a);">{{a.statusName}}<i ng-show="a.hasProcessId == 1 || a.hasAssessment != 1 || a.hasWrittenExam != 1"></i></span>
                        </td>
                    </tr>
                        <tr class="hideRow">
                            <td colspan="99">
                                <div class="hideRowCon">
                                    <div class="hideRow-left">
                                    	<div>
	                                        <button ng-click="goAssessOrExam(a,1);" ng-show="a.hasAssessment == 0" class="button button--winona" ng-class="{'sc-bgc':a.isAssessFinish == 1}" data-text="{{a.assessName}}">
	                                            <span>{{a.assessName}}</span>
	                                            <div class="after">{{a.assessName}}</div>
	                                        </button>
	                                        <button ng-click="goAssessOrExam(a,2);" ng-show="a.hasWrittenExam == 0" class="button button--winona" ng-class="{'sc-bgc':a.isWrittenExamFinish == 1}" data-text="{{a.writtenExamName}}">
	                                            <span>{{a.writtenExamName}}</span>
	                                            <div class="after">{{a.writtenExamName}}</div>
	                                        </button>
                                        </div>
                                    </div>
								<div ng-if="a.isAssessFinish == 1 && a.assessName && !a.applyStatus.length" class="hideRow-right">
									<img src="images/assessImg.png">
									<span class="need-assess">{{language.personalCenter.needAssess}}</span>
								</div>
                                <div ng-show="a.applyStatus.length" class="hideRow-right">
                                	<span class="icon iconfont icon-xiaoyuhao"></span>
                                    <div class="hideRow-right-content">
                                    	<div class="hideRow-right-all">
                                    		<div ng-repeat="m in a.applyStatus track by $index" person-flow>
		                                        <span class="HR-top">{{m.data}}</span>
		                                        <span class="dian" ng-class="{'sc-bgc': $index == 0 || m.status == 1}"></span>
		                                        <span class="xian" ng-class="{'sc-bgc xian-light': $index == 0 || m.status == 1,'xian-l0': a.applyStatus.length == 1}"></span>
		                                        <span class="HR-bottom">{{m.name}}</span>
		                                    </div>
                                    	</div>
                                    </div>
                                    <span class="icon iconfont icon-dayuhao"></span>
                                </div>
